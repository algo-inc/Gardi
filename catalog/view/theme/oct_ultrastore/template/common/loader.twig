    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const element = document.getElementById('kenguru');
            if (element) {
                element.style.left = '50%';
                let targetX = parseFloat(element.style.left);
                let direction = 'none';
                let animationFrameId = null;
                const step = 2;
                const tolerance = 100;

                function updateDirection(newDirection) {
                    if (direction !== newDirection) {
                        direction = newDirection;
                        if (direction === 'right') {
                            element.style.transform = 'translateX(-50%) scaleX(1)';
                        } else if (direction === 'left') {
                            element.style.transform = 'translateX(-50%) scaleX(-1)';
                        }
                    }
                }

                function animate() {
                    const rect = element.getBoundingClientRect();
                    const centerX = rect.left + rect.width / 2;
                    const startLimit = 100 + rect.width / 2;
                    const endLimit = window.innerWidth - 100 - rect.width / 2;
                    let newDirection = 'none';
                    if (centerX < targetX - tolerance) {
                        element.style.left = Math.min(parseInt(element.style.left) + step, endLimit - rect.width / 2) + 'px';
                        newDirection = 'right';
                    } else if (centerX > targetX + tolerance) {
                        element.style.left = Math.max(parseInt(element.style.left) - step, startLimit - rect.width / 2) + 'px';
                        newDirection = 'left';
                    }
                    updateDirection(newDirection);
                    if (newDirection !== 'none') {
                        animationFrameId = requestAnimationFrame(animate);

                    } else {
                        cancelAnimationFrame(animationFrameId);
                        animationFrameId = null;
                        element.classList.remove('moving');
                    }
                }

                function handleMouseMove(event) {
                    const rect = element.getBoundingClientRect();
                    const mouseX = event.clientX;
                    const startLimit = 100 + rect.width / 2;
                    const endLimit = window.innerWidth - 100 - rect.width / 2;
                    targetX = Math.min(Math.max(mouseX, startLimit), endLimit);
                    if (!animationFrameId) {
                        element.classList.add('moving')
                        animate();

                    }
                }

                document.addEventListener('mousemove', handleMouseMove);
            }
        });
    </script>
