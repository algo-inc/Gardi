    <script>
        let chart, chart2, chart3, chart4, chart5, chart6;
        const container = document.getElementById('hover-container');
        const left = document.querySelector('.left');
        const middle = document.querySelector('.middle');
        const right = document.querySelector('.right');
        let currentZone = '';

        container.addEventListener('mousemove', (e) => {
            const rect = container.getBoundingClientRect();
            const x = e.clientX - rect.left;
            const width = rect.width;

            if (x < width / 4) {
                if (currentZone !== 'left') {
                    leftZone();
                    currentZone = 'left';
                }
            } else if (x < (2 * width) / 3) {
                if (currentZone !== 'middle') {
                    middleZone();
                    currentZone = 'middle';
                }
            } else {
                if (currentZone !== 'right') {
                    rightZone();
                    currentZone = 'right';
                }
            }
        });

        function leftZone() {
            closeAllModals();
            renderChartsLeft();
            left.style.transform = 'translateX(0)';
            middle.style.transform = 'translateX(100%)';
            right.style.transform = 'translateX(100%)';
        }

        function middleZone() {
            closeAllModals();
            left.style.transform = 'translateX(-100%)';
            middle.style.transform = 'translateX(0)';
            right.style.transform = 'translateX(100%)';
        }

        function rightZone() {
            closeAllModals();
            renderChartsRight();
            left.style.transform = 'translateX(-100%)';
            middle.style.transform = 'translateX(-100%)';
            right.style.transform = 'translateX(0)';

        }

        function closeAllModals() {
            left.style.transform = 'translateX(-100%)';
            middle.style.transform = 'translateX(0)';
            right.style.transform = 'translateX(100%)';
        }


        function renderChartsLeft() {
            if (chart) {
                chart.destroy();
            }
            if (chart2) {
                chart2.destroy();
            }
            if (chart3) {
                chart3.destroy();
            }

            chart = new ApexCharts(document.querySelector("#chart-1"), options);
            chart2 = new ApexCharts(document.querySelector("#chart-2"), options2);
            chart3 = new ApexCharts(document.querySelector("#chart-3"), options3);

            chart.render();
            chart2.render();
            chart3.render();
        }

        function renderChartsRight() {
            if (chart4) {
                chart4.destroy();
            }
            if (chart5) {
                chart5.destroy();
            }
            if (chart6) {
                chart6.destroy();
            }
            chart4 = new ApexCharts(document.querySelector("#chart-4"), options4);
            chart5 = new ApexCharts(document.querySelector("#chart-5"), options5);
            chart6 = new ApexCharts(document.querySelector("#chart-6"), options6);

            chart4.render();
            chart5.render();
            chart6.render();
        }
    </script>
